        jtxtDisplay.setText(iNum);
    }                                     

    private void jbtnBackspaceActionPerformed(java.awt.event.ActionEvent evt) {                                              
        String bsp = null;
        
        if(jtxtDisplay.getText().length() > 0)
        {
            StringBuilder strB = new StringBuilder(jtxtDisplay.getText());
            strB.deleteCharAt(jtxtDisplay.getText().length() - 1);
            bsp = strB.toString();
            jtxtDisplay.setText(bsp);
            
            
            
        }
    }                                             

    private void jButton20ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        String answer = null;
        san2 = Double.parseDouble(jtxtDisplay.getText());
        if (operations == "+")
        {
            natije = san1 + san2;
            answer = String.format("%.1f",natije);
            jtxtDisplay.setText(answer);
        }
        else if (operations == "-")
        {
            natije = san1 - san2;
            answer = String.format("%.1f",natije);
            jtxtDisplay.setText(answer);
            
        }
        else if (operations == "*")
        {
            natije = san1 * san2;
            answer = String.format("%.1f",natije);
            jtxtDisplay.setText(answer);
            
        }
        else if (operations == "/")
        {
            natije = san1 / san2;
            answer = String.format("%.1f",natije);
            jtxtDisplay.setText(answer);
            
        }
        else if (operations == "%")
        {
            natije = san1 % san2;
            answer = String.format("%.1f",natije);
            jtxtDisplay.setText(answer);
            
        }
        else if (operations =="X^Y")
        {
        
        }
        else if(operations == "x^y"){
            Math.pow(san1, san2);
            jtxtDisplay.setText(Double.toHexString(natije));
        }
            
    }                                         

