    private void jtxtDisplayKeyTyped(java.awt.event.KeyEvent evt) {                                     
        char c =evt.getKeyChar();
        if(!(Charecter.isDigit(c) || (c==KeyEvent.VK_BACK_SPACE)
                || (c == KeyEvent.VK_DELETE))){
            getToolkit().beep();
            evt.consume();
        }
    }                                    

